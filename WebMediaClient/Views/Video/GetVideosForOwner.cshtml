@model List<WebMediaClient.Models.VideoViewModel>

@{
    ViewBag.Title = "GetVideosForOwner";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>GetVideosForOwner</h2>

<div class="row">
	<div class="col-md-8">
		<div class="list-group">
			@foreach (var item in Model)
			{
				<div class="list-group-item">
					@Video.Flash(item.Location,
								"400",
								"500",
								false,
								false,
								false,
								"blue",
								"high",
								"exactfit",
								"transparent")
				</div>
			}
		</div>
	</div>
</div>

